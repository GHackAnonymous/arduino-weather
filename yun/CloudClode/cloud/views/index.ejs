          },
          data: [//array of dataSeries
            { //dataSeries object
              name: "Temperatura",
             /*** Change type "column" to "bar", "area", "line" or "pie"***/
             type: "line",
              dataPoints: [ <%= datosTemperatura %> ]
           },
           { //dataSeries object
             name: "Humedad",
            /*** Change type "column" to "bar", "area", "line" or "pie"***/
            type: "line",
             dataPoints: [ <%= datosHumedad %> ]
          }
           ]
         });

        chart.render();
      }
      </script>
